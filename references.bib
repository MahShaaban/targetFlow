issn = {16643224},
journal = {Frontiers in Immunology},
keywords = {Dendritic cell,Immunotherapy,Infection,Macrophage,Monocyte,Mononuclear phagocyte system,Pattern recognition receptors,Tuberculosis},
month = {feb},
number = {FEB},
pmid = {29479353},
title = {{Reinforcing the functionality of mononuclear phagocyte system to control tuberculosis}},
url = {http://journal.frontiersin.org/article/10.3389/fimmu.2018.00193/full},
volume = {9},
year = {2018}
}
