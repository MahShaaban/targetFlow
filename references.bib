pmid = {21317285},
title = {{WIPI1 coordinates melanogenic gene transcription and melanosome formation via TORC1 inhibition}},
url = {http://www.jbc.org/lookup/doi/10.1074/jbc.M110.200543 http://www.ncbi.nlm.nih.gov/pubmed/21317285 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3069453},
