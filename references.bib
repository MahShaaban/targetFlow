title = {{Resistance to inhibitors of cholinesterase (Ric)-8A and G$\alpha${\textless}inf{\textgreater}i{\textless}/inf{\textgreater} contribute to cytokinesis abscission by controlling Vacuolar protein-sorting (Vps)34 activity}},
url = {http://dx.plos.org/10.1371/journal.pone.0086680 http://www.ncbi.nlm.nih.gov/pubmed/24466196 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3897744},
