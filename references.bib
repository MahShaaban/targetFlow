title = {{Peroxiredoxin 1 promotes invasion and migration by regulating epithelial-to-mesenchymal transition during oral carcinogenesis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/27259998{\%}0Ahttp://www.ncbi.nlm.nih.gov/pubmed/27259998},
