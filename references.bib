keywords = {Adipogenesis/genetics/*physiology,Animals,Autophagy/genetics/*physiology,Biological,Diabetes Mellitus,Humans,Mice,Models,Obesity/genetics/pathology/*physiopathology,Type 2/genetics/pathology/*phys,adipose,atg7,knockout,obe-},
