pages = {5399--5404},
pmid = {29552183},
title = {{Macrophages in skin melanoma-the key element in melanomagenesis (Review)}},
url = {http://www.spandidos-publications.com/10.3892/ol.2018.8021},
volume = {15},
year = {2018}
}
@article{Li2016a,
abstract = {HIV-1 latency is mainly characterized by a reversible silencing of long-terminal repeat (LTR)-driven transcription of provirus. The existing of repressive factors has been described to contribute to transcription silencing of HIV-1. Toll-interacting protein (Tollip) has been identified as a repressor of Toll like receptors (TLR)-mediated signaling. Our previous study has found that Tollip inhibited NF-$\kappa$B-dependent HIV-1 promoter LTR-driven transcription, indicating the potential role of Tollip in governing viral latency. In this study, by using HIV-1 latently infected Jurkat T-cell and central memory CD4+T-cells, we demonstrate the role of Tollip in regulating HIV-1 latency, as the knock-down of Tollip promoted HIV-1 reactivation from both HIV-1 latently infected Jurkat CD4+T cells and primary central memory T cells (TCM). Moreover, we found that the activities of LTRs derived from multiple HIV-1 subtypes could be repressed by Tollip; Knock-down of Tollip promoted HIV-1 transcription and infection in CD4+T cells. Our data indicate a key role of Tollip in suppressing HIV-1 infection and regulating viral latency, which provides a potential host target for combating HIV-1 infection and latency.},
author = {Li, Chuan and Kuang, Wen Dong and Qu, Di and Wang, Jian Hua},
doi = {10.1016/j.bbrc.2016.05.065},
issn = {10902104},
journal = {Biochemical and Biophysical Research Communications},
keywords = {HIV-1,Long-terminal repeat,Toll-interacting protein,Viral latency},
month = {jun},
number = {2},
pages = {161--168},
pmid = {27181351},
title = {{Toll-interacting protein inhibits HIV-1 infection and regulates viral latency}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0006291X16307574 http://www.ncbi.nlm.nih.gov/pubmed/27181351},
volume = {475},
year = {2016}
}
@article{Bodenstine2008,
abstract = {The most lethal and debilitating attribute of cancer cells is their ability to metastasize. Throughout the process of metastasis, tumor cells interact with other tumor cells, host cells and a variety of molecules. Tumor cells are also faced with a number of insults, such as hemodynamic sheer pressure and immune selection. This brief review explores how metastasis suppressor proteins regulate interactions between tumor cells and the microenvironments in which tumor cells find themselves. {\textcopyright} 2010 Elsevier Ltd.},
