journal = {Oncotarget},
keywords = {AKT,Antibiotics,Antineoplastic,Antineoplastic Agents,Antineoplastic: pharmacology,Breast Neoplasms,Breast Neoplasms: enzymology,Breast Neoplasms: genetics,Breast Neoplasms: pathology,Cell Aging,Cell Aging: drug effects,Cell Line,Dose-Response Relationship,Doxorubicin,Doxorubicin: pharmacology,Drug,Drug Resistance,Female,Hormonal,Hormonal: pharmacology,Humans,Neoplasm,Proto-Oncogene Proteins c-akt,Proto-Oncogene Proteins c-akt: genetics,Proto-Oncogene Proteins c-akt: metabolism,Proto-Oncogene Proteins c-raf,Proto-Oncogene Proteins c-raf: genetics,Proto-Oncogene Proteins c-raf: metabolism,Raf,Signal Transduction,Signal Transduction: drug effects,Tamoxifen,Tamoxifen: pharmacology,Time Factors,Transfection,Tumor,breast},
