keywords = {Dissemination,Epithelial plasticity,Epithelial-mesenchymal transition,Metastasis,Prostate cancer,Stem cell,chemoresistance,prostate,review},
mendeley-tags = {chemoresistance,prostate,review},
month = {sep},
