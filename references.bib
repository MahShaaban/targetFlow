mendeley-tags = {MicroRNA,breast,review},
number = {12},
pages = {130},
pmid = {28567067},
title = {{Therapeutic impacts of microRNAs in breast cancer by their roles in regulating processes involved in this disease}},
url = {http://www.jmsjournal.net/text.asp?2017/22/1/130/221520},
volume = {22},
year = {2017}
}
