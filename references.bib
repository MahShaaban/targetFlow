journal = {EMBO Journal},
keywords = {LIN28,Let-7,MAP kinase,Metastasis,RKIP,Raf kinase inhibitory protein,metastasis},
mendeley-tags = {RKIP,metastasis},
month = {feb},
number = {4},
pages = {347--358},
pmid = {19153603},
title = {{Raf kinase inhibitory protein suppresses a metastasis signalling cascade involving LIN28 and let-7}},
url = {http://emboj.embopress.org/cgi/doi/10.1038/emboj.2008.294},
volume = {28},
year = {2009}
}
