isbn = {1748-7838 (Electronic)$\backslash$r1001-0602 (Linking)},
issn = {17487838},
journal = {Cell Research},
keywords = {FTO,METTL3,N6-methyladenosine (m6A),adipogenesis,mRNA splicing},
month = {dec},
number = {12},
pages = {1403--1419},
pmid = {25412662},
title = {{FTO-dependent demethylation of N6-methyladenosine regulates mRNA splicing and is required for adipogenesis}},
volume = {24},
