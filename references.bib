mendeley-tags = {GIT,apoptosis,chemoresistance},
month = {oct},
pages = {3798--3803},
pmid = {27755523},
title = {{Paris Saponin I Sensitizes Gastric Cancer Cell Lines to Cisplatin via Cell Cycle Arrest and Apoptosis.}},
url = {http://www.medscimonit.com/abstract/index/idArt/898232 http://www.ncbi.nlm.nih.gov/pubmed/27755523 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5081239},
