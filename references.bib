pages = {99--115},
pmid = {14701881},
title = {{Notch promotes epithelial-mesenchymal transition during cardiac development and oncogenic transformation}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/14701881 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC314285 http://www.genesdev.org/cgi/doi/10.1101/gad.276304},
