pmid = {28790197},
title = {{Breast tumors educate the proteome of stromal tissue in an individualized but coordinated manner}},
url = {http://stke.sciencemag.org/lookup/doi/10.1126/scisignal.aam8065},
volume = {10},
year = {2017}
}
@article{Liu2015,
abstract = {{\textless}p{\textgreater} Breast cancer metastasis suppressor 1 (BRMS1) is downregulated in non-small cell lung cancer (NSCLC), and its reduction correlates with disease progression. Herein, we investigate the mechanisms through which loss of the {\textless}italic{\textgreater}BRMS1{\textless}/italic{\textgreater} gene contributes to epithelial-to-mesenchymal transition (EMT). Using a short hairpin RNA (shRNA) system, we show that loss of BRMS1 promotes basal and transforming growth factor beta-induced EMT in NSCLC cells. NSCLC cells expressing {\textless}italic{\textgreater}BRMS1{\textless}/italic{\textgreater} shRNAs ( {\textless}italic{\textgreater}BRMS1{\textless}/italic{\textgreater} knockdown [ {\textless}italic{\textgreater}BRMS1{\textless}/italic{\textgreater} {\textless}sup{\textgreater}KD{\textless}/sup{\textgreater} ]) display mesenchymal characteristics, including enhanced cell migration and differential regulation of the EMT markers. Mesenchymal phenotypes observed in {\textless}italic{\textgreater}BRMS1{\textless}/italic{\textgreater} {\textless}sup{\textgreater}KD{\textless}/sup{\textgreater} cells are dependent on RelA/p65, the transcriptionally active subunit of nuclear factor kappa B (NF-$\kappa$B). In addition, chromatin immunoprecipitation analysis demonstrates that loss of {\textless}italic{\textgreater}BRMS1{\textless}/italic{\textgreater} increases {\textless}italic{\textgreater}Twist1{\textless}/italic{\textgreater} promoter occupancy of RelA/p65 K310—a key histone modification associated with increased transcription. Knockdown of {\textless}italic{\textgreater}Twist1{\textless}/italic{\textgreater} results in reversal of {\textless}italic{\textgreater}BRMS1{\textless}/italic{\textgreater} {\textless}sup{\textgreater}KD{\textless}/sup{\textgreater} -mediated EMT phenotypic changes. Moreover, in our animal model, {\textless}italic{\textgreater}BRMS1{\textless}/italic{\textgreater} {\textless}sup{\textgreater}KD{\textless}/sup{\textgreater} / {\textless}italic{\textgreater}Twist1{\textless}/italic{\textgreater} {\textless}sup{\textgreater}KD{\textless}/sup{\textgreater} double knockdown cells were less efficient in establishing lung tumors than {\textless}italic{\textgreater}BRMS1{\textless}/italic{\textgreater} {\textless}sup{\textgreater}KD{\textless}/sup{\textgreater} cells. Collectively, this study demonstrates that loss of BRMS1 promotes malignant phenotypes that are dependent on NF-$\kappa$B-dependent regulation of {\textless}italic{\textgreater}Twist1{\textless}/italic{\textgreater} . These observations offer fresh insight into the mechanisms through which BRMS1 regulates the development of metastases in NSCLC. {\textless}/p{\textgreater}},
