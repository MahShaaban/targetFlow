doi = {10.1177/1535370215619041},
file = {:Users/mahmoudahmed/Mendeley Desktop/Poulos et al. - 2016 - The increasingly complex regulation of adipocyte differentiation.pdf:pdf},
issn = {15353699},
journal = {Experimental Biology and Medicine},
keywords = {Adipocyte,differentiation,extracellular matrix,hypoxia,preadipocyte,stromal vascular},
number = {5},
pages = {449--456},
