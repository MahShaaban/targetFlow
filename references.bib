pmid = {29191657},
title = {{Extracellular vesicle-encapsulated microRNA-761 enhances pazopanib resistance in synovial sarcoma}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0006291X17323410 http://www.ncbi.nlm.nih.gov/pubmed/29191657},
