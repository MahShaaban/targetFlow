pmid = {21126221},
title = {{Autophagic Degradation of Mitochondria in White Adipose Tissue Differentiation}},
url = {http://www.liebertonline.com/doi/abs/10.1089/ars.2010.3777},
volume = {14},
year = {2011}
}
