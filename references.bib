pmid = {29536823},
title = {{Ensemble of rankers for efficient gene signature extraction in smoke exposure classification}},
url = {https://bmcbioinformatics.biomedcentral.com/articles/10.1186/s12859-018-2035-3},
volume = {19},
year = {2018}
