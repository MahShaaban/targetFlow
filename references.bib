issn = {02614189},
journal = {EMBO Journal},
keywords = {Animals,Autophagy,Biological Clocks,C/EBP$\beta$,CCAAT-Enhancer-Binding Protein-beta,Cells,Circadian Rhythm,Cultured,Gene Expression,Gene Expression Regulation,Genetic,Hepatocytes,Inbred C57BL,Knockout,Liver,Male,Mice,Promoter Regions,Proteolysis,RNA,Small Interfering,autophagy,circadian rhythm,clock,metabolism},
month = {sep},
number = {22},
pages = {4642--4651},
pmid = {21897364},
title = {{Temporal orchestration of circadian autophagy rhythm by C/EBP$\beta$}},
volume = {30},
year = {2011}
}
@article{Lee2019,
abstract = {Abstract Recent studies suggest that, even within a single adipose depot, there may be distinct subpopulations of adipocytes. To investigate this cellular heterogeneity, we have developed multiple conditionally immortalized clonal preadipocyte lines from white adipose tissue of mice. Analysis of these clones reveals at least three white adipocyte subpopulations. These subpopulations have differences in metabolism and differentially respond to inflammatory cytokines, insulin, and growth hormones. These also have distinct gene expression profiles and can be tracked by differential expression of three marker genes: Wilms' tumor 1, transgelin, and myxovirus 1. Lineage tracing analysis with dual‐fluorescent reporter mice indicates that these adipocyte subpopulations have differences in gene expression and metabolism that mirror those observed in the clonal cell lines. Furthermore, preadipocytes and adipocytes from these subpopulations differ in their abundance in different fat depots. Thus, white adipose tissue, even in a single depot, is comprised of distinct subpopulations of white adipocytes with different physiological phenotypes. These differences in adipocyte composition may contribute to the differences in metabolic behavior and physiology of different fat depots.},
author = {Lee, Kevin Y and Luong, Quyen and Sharma, Rita and Dreyfuss, Jonathan M and Ussar, Siegfried and Kahn, C Ronald},
doi = {10.15252/embj.201899291},
file = {:Users/mahmoudahmed/Mendeley Desktop/EMBJ-38-e99291.pdf:pdf},
