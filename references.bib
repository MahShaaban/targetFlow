pmid = {27880917},
title = {{Phenotypic and Interaction Profiling of the Human Phosphatases Identifies Diverse Mitotic Regulators}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S2211124716315133 http://www.ncbi.nlm.nih.gov/pubmed/27880917},
