keywords = {2016,2016 accepted,2016 published,Snail,androgen receptor,april 29,castration resistance,chemoresistance,enzalutamide,july 7,june 20,metastasis,prostate,received,snail},
mendeley-tags = {Snail,chemoresistance,prostate},
month = {aug},
number = {31},
pages = {50507--50521},
pmid = {27409172},
title = {{Snail promotes resistance to enzalutamide through regulation of androgen receptor activity in prostate cancer}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26517521{\%}0Ahttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4811456 http://www.ncbi.nlm.nih.gov/pubmed/27409172 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5226599 http://www.oncotarget.com/f},
