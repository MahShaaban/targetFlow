doi = {10.1080/21623945.2017.1319536},
file = {:Users/mahmoudahmed/Mendeley Desktop/Boumelhem et al. - 2017 - Flow cytometric single cell analysis reveals heterogeneity between adipose depots.pdf:pdf},
issn = {2162397X},
