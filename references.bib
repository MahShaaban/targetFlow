arxivId = {1512.00567},
author = {Lu, Jun and Getz, Gad and Miska, Eric A. and Alvarez-Saavedra, Ezequiel and Lamb, Justin and Peck, David and Sweet-Cordero, Alejandro and Ebert, Benjamin L. and Mak, Raymond H. and Ferrando, Adolfo A. and Downing, James R. and Jacks, Tyler and Horvitz, H. Robert and Golub, Todd R.},
