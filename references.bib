journal = {Aging},
keywords = {AKT,Apoptosis,Cancer,Kinases,MEK,MTOR,PI3K,Protein phosphorylation,RAF,Raf,Signal transduction,tumor},
mendeley-tags = {AKT,PI3K,Raf,tumor},
