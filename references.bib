journal = {European Journal of Medical Genetics},
keywords = {Beta-propeller protein-associated neurodegeneratio,Brain atrophy,Epileptic encephalopathy,Neurodegeneration with brain iron accumulation,Neuron specific enolase (NSE),WDR45},
month = {oct},
number = {10},
pages = {521--526},
pmid = {28711740},
title = {{Early manifestations of epileptic encephalopathy, brain atrophy, and elevation of serum neuron specific enolase in a boy with beta-propeller protein-associated neurodegeneration}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1769721217302203 http://www.ncbi.nlm.nih.gov/pubmed/28711740},
volume = {60},
year = {2017}
}
@article{Pieniazek2018,
abstract = {Cutaneous melanoma is an aggressive cancer and its onset and growth are associated, through direct and indirect interactions, with the cancer microenvironment. The microenvironment comprises a dynamic complex of numerous types of cells (due to histogenesis) that constantly interact with each other through multiple cytokines and signaling proteins. Macrophages are one of the most thoroughly studied pleiotropic cells of the immune system. One of their major cytophysiological functions is their involvement in phagocytosis. Previous studies examining the microenvironment of melanomas and tumor-associated macrophages have revealed that they are involved in all stages of melanomagenesis. In the case of cancer initiation, they form an inflammatory microenvironment and then suppress the anticancer activity of the immune system, stimulate angiogenesis, enhance migration and invasion of the cancer cells, and ultimately contribute to the metastatic process. The present review provides a detailed overview on the function of macrophages in the melanoma microenvironment.},
author = {Pieniazek, Malgorzata and Matkowski, Rafal and Donizy, Piotr},
doi = {10.3892/ol.2018.8021},
issn = {17921082},
journal = {Oncology Letters},
keywords = {Macrophage,Melanoma,Microenvironment,Oncoimmunology,Tumor-associated macrophage},
month = {feb},
