pages = {E4288--E4297},
pmid = {26187928},
title = {{MicroRNA-224 promotes tumor progression in nonsmall cell lung cancer}},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1502068112},
volume = {112},
year = {2015}
