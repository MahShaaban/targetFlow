pages = {E11151--E11160},
pmid = {29229824},
title = {{Molecular mechanism for the subversion of the retromer coat by the {\textless}i{\textgreater}Legionella{\textless}/i{\textgreater} effector RidL}},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1715361115},
volume = {114},
