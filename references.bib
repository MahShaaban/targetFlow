issn = {14764679},
journal = {Nature Cell Biology},
number = {7},
pages = {782--788},
publisher = {Springer US},
title = {{Aspartate is an endogenous metabolic limitation for tumour growth}},
url = {http://dx.doi.org/10.1038/s41556-018-0125-0},
volume = {20},
year = {2018}
}
