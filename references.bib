issn = {1742464X},
journal = {The FEBS Journal},
keywords = {3T3-L1 Cells,Adipocytes,Adipogenesis,Animals,B-Lymphocytes,CCAAT-Enhancer-Binding Protein-beta,CCAAT-Enhancer-Binding Proteins,Cell Differentiation,Chromatin,Gene Expression Profiling,Gene Expression Regulation,Genetic,Histones,Interferon-gamma,Mice,Nuclear Proteins,Organ Specificity,PPAR gamma,Phosphatidate Phosphatase,Primary Cell Culture,Retinoid X Receptors,Signal Transduction,Transcription,adipogenesis,chromosome conformation,transcription,transcription factor},
