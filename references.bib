author = {Levovitz, Chaya and Chen, Dan and Ivansson, Emma and Gyllensten, Ulf and Finnigan, John P. and Alshawish, Sara and Zhang, Weijia and Schadt, Eric E. and Posner, Marshal R. and Genden, Eric M. and Boffetta, Paolo and Sikora, Andrew G.},
