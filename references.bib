journal = {Frontiers in Cellular Neuroscience},
month = {jan},
pmid = {26778963},
title = {{$\beta$-Secretase 1's Targeting Reduces Hyperphosphorilated Tau, Implying Autophagy Actors in 3xTg-AD Mice}},
url = {http://journal.frontiersin.org/Article/10.3389/fncel.2015.00498/abstract},
