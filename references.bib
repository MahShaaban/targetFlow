month = {sep},
number = {9},
pages = {1023--1037},
pmid = {16923123},
title = {{Screening for target Rabs of TBC (Tre-2/Bub2/Cdc16) domain-containing proteins based on their Rab-binding activity}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16923123},
volume = {11},
year = {2006}
