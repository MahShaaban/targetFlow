issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
month = {mar},
number = {13},
pages = {5117--22},
pmid = {19282476},
title = {{Genome-wide discovery of functional transcription factor binding sites by comparative genomics: the case of Stat3.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19282476 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2664024},
volume = {106},
