journal = {Molecular and cellular biology},
keywords = {3T3-L1 Cells,Adipocytes,Adipogenesis,Animals,Autophagy,Autophagy-Related Proteins,CCAAT-Enhancer-Binding Protein-beta,Cysteine Endopeptidases,Kruppel-Like Transcription Factors,Mice,Proteolysis,Transcription Factors,Transcriptional Activation},
