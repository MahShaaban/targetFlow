issn = {2041-4889},
journal = {Cell death {\&} disease},
keywords = {NPC,apoptosis,lncRNA,radioresistance},
mendeley-tags = {NPC,apoptosis,lncRNA,radioresistance},
month = {feb},
number = {2},
pages = {235},
pmid = {29445147},
title = {{Long non-coding RNA PVT1 predicts poor prognosis and induces radioresistance by regulating DNA repair and cell apoptosis in nasopharyngeal carcinoma.}},
url = {http://www.nature.com/articles/s41419-018-0265-y http://www.ncbi.nlm.nih.gov/pubmed/29445147 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5833381},
volume = {9},
