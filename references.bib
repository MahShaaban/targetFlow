issn = {14602156},
journal = {Brain},
keywords = {NBIA,Rett syndrome,autophagy,basal ganglia,iron},
month = {jun},
number = {6},
pages = {1708--1717},
pmid = {23687123},
title = {{Beta-propeller protein-associated neurodegeneration: A new X-linked dominant disorder with brain iron accumulation}},
url = {https://academic.oup.com/brain/article-lookup/doi/10.1093/brain/awt095 http://www.ncbi.nlm.nih.gov/pubmed/23687123 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3673459},
