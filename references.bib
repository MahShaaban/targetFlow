title = {{Clinicopathological features and prognostic implications of Raf kinase inhibitor protein downregulation in tongue squamous cell carcinoma}},
url = {https://www.spandidos-publications.com/10.3892/ol.2015.3496 http://www.ncbi.nlm.nih.gov/pubmed/26622668 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4533748},
volume = {10},
