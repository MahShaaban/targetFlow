issn = {17921082},
journal = {Oncology Letters},
keywords = {Cholangiocarcinoma,Invasion,Migration,RKIP,Raf kinase inhibitor protein,cholangiocarcinoma,metastasis},
mendeley-tags = {RKIP,cholangiocarcinoma,metastasis},
