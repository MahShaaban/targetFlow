journal = {PLoS Biology},
month = {jan},
number = {1},
pages = {e2004411},
pmid = {29381698},
title = {{Role of the AP-5 adaptor protein complex in late endosome-to-Golgi retrieval}},
url = {http://dx.plos.org/10.1371/journal.pbio.2004411},
volume = {16},
year = {2018}
}
