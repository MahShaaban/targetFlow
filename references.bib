pages = {e0172399},
pmid = {28253359},
title = {{CSB ablation induced apoptosis is mediated by increased endoplasmic reticulum stress response}},
url = {http://dx.plos.org/10.1371/journal.pone.0172399 http://www.ncbi.nlm.nih.gov/pubmed/28253359 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5333825},
volume = {12},
