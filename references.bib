isbn = {1097-0045 (Electronic)$\backslash$r0270-4137 (Linking)},
issn = {02704137},
journal = {Prostate},
keywords = {Slug,benign hyperplasia,epithelial-mesenchymal transition,miR-200 family,transforming growth factor-$\beta$1},
month = {sep},
