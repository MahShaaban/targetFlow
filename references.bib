pages = {214--226},
pmid = {18439900},
title = {{AMPK Phosphorylation of Raptor Mediates a Metabolic Checkpoint}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18439900 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2674027 http://linkinghub.elsevier.com/retrieve/pii/S109727650800169X},
