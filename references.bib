@article{Fan2013,
abstract = {The Cockayne syndrome complementation group B protein, CSB, plays pivotal roles in transcription regulation and DNA repair. CSB belongs to the SNF2/SWI2 ATP-dependent chromatin remodeling protein family, and studies from many laboratories have revealed that CSB has multiple activities and modes of regulation. To understand the underlying mechanisms of Cockayne syndrome, it is necessary to understand how the biochemical activities of CSB are used to carry out its biological functions. In this review, we summarize our current knowledge of the structure, function and regulation of CSB, and discuss how these properties can impact the biological functions of this chromatin remodeler. {\textcopyright} 2013 Elsevier Ireland Ltd.},
