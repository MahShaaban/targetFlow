issn = {14712350},
journal = {BMC Medical Genetics},
keywords = {Alternative splicing,Osteoclast,Paget's disease of bone,TBC1D25,bone,p62/SQSTM1,splicing},
mendeley-tags = {TBC1D25,bone,splicing},
month = {dec},
