isbn = {0040-8891 (Print)$\backslash$r0040-8891},
issn = {0040-8891},
journal = {The Bulletin of Tokyo Dental College},
keywords = {AKT,PI3K,cdna microarray,early cancer,epithelial dysplasia,pi3k-akt,tongue},
mendeley-tags = {AKT,PI3K,tongue},
month = {aug},
number = {3},
pages = {125--133},
pmid = {19887755},
title = {{Activation of PI3K-AKT pathway in oral epithelial dysplasia and early cancer of tongue.}},
