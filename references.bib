@article{Wei2017,
abstract = {Raf kinase inhibitory protein (RKIP) regulates multiple cellular processes, and its downregulation is associated with distinct human cancers. In the present study, the status of RKIP promoter methylation, as well as its expression and clinical significance in esophageal squamous cell carcinoma (ESCC), were examined. The promoter methylation status in the 5'-CpG island of the RKIP gene and the expression level of the RKIP protein were examined using a modified methylation-specific polymerase chain reaction (MSP) method and immunohistochemical staining, respectively, in 77 ESCC samples and matched paratumor normal tissues. The incidence of RKIP promoter methylation was significantly higher in tumor samples (75.3{\%}) than in the matched normal tissues (27.3{\%}; P{\textless}0.001). A higher incidence of promoter methylation was also detected in poorly differentiated cancers (93.5{\%}) compared with well-differentiated cancers (50.0{\%}; P{\textless}0.001), as well as in tumor samples with positive lymph node metastasis (86.7{\%}) compared with those with negative lymph node metastasis (59.4{\%}; P{\textless}0.001). Consistent with the promoter methylation status, the expression level of RKIP was significantly reduced in cancer tissues (36.4{\%}) compared with matched normal tissues (76.6{\%}; P{\textless}0.01), as well as in cancers with positive lymph node metastasis (24.4{\%}) compared with those with negative lymph node metastasis (53.1{\%}; P=0.01). Promoter methylation-induced gene silencing significantly correlated with the down regulation of RKIP and the development of ESCC. The results of the present study suggested that the methylation status of the RKIP promoter, when combined with its expression level, may serve as a biomarker for predicting the biological behaviors of ESCC.},
author = {Wei, Hong and Liu, Zhijun and She, Hongyan and Liu, Baoguo and Gu, Junxia and Wei, Dongmin and Zhang, Xiangyang and Wang, Jiufeng and Qi, Shujing and Ping, Fumin},
doi = {10.3892/ol.2017.5617},
isbn = {1792-1074 (Print)1792-1074 (Linking)},
issn = {17921082},
journal = {Oncology Letters},
keywords = {DNA methylation,Esophageal squamous cell carcinoma,HM,Lymph node metastasis,RKIP,Raf kinase inhibitory protein,Tumor differentiation,esophagus},
mendeley-tags = {HM,RKIP,esophagus},
month = {mar},
number = {3},
pages = {1866--1872},
pmid = {28454336},
title = {{Promoter methylation and expression of Raf kinase inhibitory protein in esophageal squamous cell carcinoma}},
url = {https://www.spandidos-publications.com/10.3892/ol.2017.5617 http://www.ncbi.nlm.nih.gov/pubmed/28454336 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5403527},
volume = {13},
