isbn = {1532-1827 (Electronic)0007-0920 (Linking)},
issn = {15321827},
journal = {British Journal of Cancer},
keywords = {KRAS,chemoresistance,chemotherapy,colorectal,colorectal adenoma,colorectal cancer,miR-224,microRNA},
mendeley-tags = {chemoresistance,colorectal,microRNA},
month = {apr},
number = {9},
pages = {1480--1490},
pmid = {25919696},
title = {{MicroRNA-224 is associated with colorectal cancer progression and response to 5-fluorouracil-based chemotherapy by KRAS-dependent and-independent mechanisms}},
