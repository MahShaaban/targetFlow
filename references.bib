arxivId = {NIHMS150003},
author = {Baritaki, Stavroula and Huerta-Yepez, Sara and Cabrava-Haimandez, Ma da Lourdas and Sensi, Marialuisa and Canevari, Silvana and Libra, Massimo and Penichet, Manuel and Chen, Haiming and Berenson, James R and Bonavida, Benjamin},
doi = {10.1615/ForumImmunDisTher.v2.i2.90},
