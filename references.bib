pmid = {25592411},
title = {{Analysis of the C19orf12 and WDR45 genes in patients with neurodegeneration with brain iron accumulation}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0022510X14008016 http://www.ncbi.nlm.nih.gov/pubmed/25592411},
