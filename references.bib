journal = {Cell Death and Disease},
month = {sep},
number = {9},
pages = {e2372--e2372},
pmid = {27899819},
title = {{4-cholesten-3-one suppresses lung adenocarcinoma metastasis by regulating translocation of HMGB1, HIF1$\alpha$ and Caveolin-1}},
url = {http://www.nature.com/articles/cddis2016281},
volume = {7},
year = {2016}
}
