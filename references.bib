journal = {Journal of Biological Chemistry},
keywords = {3T3-L1 Cells,Adipocyte,Adipocytes,Animals,DNA-Binding Proteins,Diabetes Mellitus,Gene Expression Regulation,Glucose,Glucose Transport,Glucose Transporter Type 4,Glucose Transporter Type 4 (GLUT4),Humans,Insulin,Messenger,Mice,PPAR gamma,Peroxisome Proliferator-activated Receptor (PPAR),RNA,Signal Transduction,Transcription Factors,Transcription Regulation,Type 2,Type 2 Diabetes,Zinc Finger,mRNA Decay},
month = {mar},
