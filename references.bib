month = {sep},
number = {9},
pages = {2039--2045},
pmid = {18566875},
title = {{Dominant negative LRP5 decreases tumorigenicity and metastasis of osteosarcoma in an animal model}},
url = {http://link.springer.com/10.1007/s11999-008-0344-y},
volume = {466},
year = {2008}
}
@article{Martinho2017,
abstract = {Background and Aims: There are no effective treatments for advanced cervical cancer. Receptor tyrosine kinases (RTK) inhibitors are successful drugs for a large variety of tumors, yet its efficacy in cervical cancer is unclear. The aim of this work was to evaluate the role of alterations on HER family receptors in cervical cancer patients and to assess the effect of its pharmacological inhibition. Methods: We characterized the expression of HER family by immunohistochemistry in a series of 231 tumors. The results were further validated by in silico analysis. The anti-tumorigenic effect of different pan-RTK inhibitors was evaluated in different in vitro models and in vivo approaches. Results: HER2 was overexpressed and associated with patients' poor prognosis, as corroborated by in silico analysis. Also a good correlation was observed between HER2 amplification and its mRNA and protein overexpression. By in vitro and in vivo assays, we found that HER receptors are specific targetable proteins to fighting cervical cancer cellular aggressiveness, mainly with lapatinib and allitinib pan-HER inhibitors. Moreover, we anticipated that a bypass in cellular metabolism could underlie drug resistance, mainly to lapatinib, which can be reverted by combination with glucose uptake inhibitors. Conclusions: This is first comparative study of HER family receptors expression in a large series of cervical carcinomas, and we found HER2 expression as an independent prognostic marker for this patients. We also demonstrated for the first time that HER targeted therapies alone or in combination with glucose metabolism inhibitors, are effective in the blockage of cervical cancer cells aggressiveness.},
author = {Martinho, Olga and Silva-Oliveira, Renato and Cury, Fernanda P. and Barbosa, Ana Martins and Granja, Sara and {Adriane Feij{\"{i}}¿½ Evangelista} and Marques, F{\"{i}}¿½bio and Miranda-Gon{\"{i}}¿½alves, Vera and Cardoso-Carneiro, Diana and Paula, Fl{\"{i}}¿½via E.de and Zanon, Maicon and Scapulatempo-Neto, Cristovam and Moreira, Marise A.R. and Baltazar, F{\"{i}}¿½tima and Longatto-Filho, Adhemar and Reis, Rui Manuel},
doi = {10.7150/thno.17154},
issn = {18387640},
journal = {Theranostics},
keywords = {Cervical cancer,ErbB, EGFR,HER2,Prognosis,Targeted therapy},
number = {3},
pages = {717--732},
pmid = {28255362},
title = {{HER family receptors are important theranostic biomarkers for cervical cancer: Blocking glucose metabolism enhances the therapeutic effect of HER inhibitors}},
url = {http://www.thno.org/v07p0717.htm},
volume = {7},
year = {2017}
}
@article{Cui2012,
abstract = {{\textless}p{\textgreater}Aging is an intricate phenomenon characterized by progressive decline in physiological functions and increase in mortality that is often accompanied by many pathological diseases. Although aging is almost universally conserved among all organisms, the underlying molecular mechanisms of aging remain largely elusive. Many theories of aging have been proposed, including the free-radical and mitochondrial theories of aging. Both theories speculate that cumulative damage to mitochondria and mitochondrial DNA (mtDNA) caused by reactive oxygen species (ROS) is one of the causes of aging. Oxidative damage affects replication and transcription of mtDNA and results in a decline in mitochondrial function which in turn leads to enhanced ROS production and further damage to mtDNA. In this paper, we will present the current understanding of the interplay between ROS and mitochondria and will discuss their potential impact on aging and age-related diseases.{\textless}/p{\textgreater}},
author = {Cui, Hang and Kong, Yahui and Zhang, Hong},
doi = {10.1155/2012/646354},
isbn = {2090-1747 (Electronic)$\backslash$r2090-1747 (Linking)},
issn = {2090-1739},
journal = {Journal of Signal Transduction},
pages = {1--13},
pmid = {21977319},
title = {{Oxidative Stress, Mitochondrial Dysfunction, and Aging}},
