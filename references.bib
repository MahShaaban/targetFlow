isbn = {6176321972},
issn = {1557-7988},
journal = {Methods in enzymology},
keywords = {Adipocyte,Adipose tissue,Flow cytometry,Flow sorting,Obesity},
number = {1},
pages = {281--96},
pmid = {24480352},
title = {{Analysis and isolation of adipocytes by flow cytometry.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24480352 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4143162},
volume = {537},
year = {2014}
}
