journal = {Oncotarget},
keywords = {Autophagy,LC3,PI3K,RKIP,RUNX2,TF,melanoma,receptor tyrosine kinase,resistance to targeted therapy,transcription factor},
mendeley-tags = {Autophagy,LC3,PI3K,RKIP,TF,melanoma},
