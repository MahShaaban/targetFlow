@article{Zebisch2012,
abstract = {In this note, we present a detailed procedure for highly effective and reproducible 3T3-L1 cell differentiation. Due to their potential to differentiate from fibroblasts to adipocytes, 3T3-L1 cells are widely used for studying adipogenesis and the biochemistry of adipocytes. However, using different kits and protocols published so far, we were not able to obtain full differentiation of the currently available American Type Culture Collection (ATCC) 3T3-L1 cell lots. Using rosiglitazone (2 $\mu$M) as an additional prodifferentiative agent, we achieved apparently complete differentiation of 3T3-L1 cells within 10 to 12 days that persisted for at least up to cell culture passage 10. {\textcopyright} 2012 Elsevier Inc. All rights reserved.},
