pages = {708--716},
pmid = {23761631},
title = {{RKIP Contributes to IFN-  Synthesis by CD8+ T Cells after Serial TCR Triggering in Systemic Inflammatory Response Syndrome}},
url = {http://www.jimmunol.org/cgi/doi/10.4049/jimmunol.1203486},
volume = {191},
year = {2013}
}
