pages = {424--432},
pmid = {26845057},
title = {{MicroRNA-761 is upregulated in hepatocellular carcinoma and regulates tumorigenesis by targeting Mitofusin-2}},
url = {http://doi.wiley.com/10.1111/cas.12904 http://www.ncbi.nlm.nih.gov/pubmed/26845057 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4832850},
