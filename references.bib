issn = {20452322},
journal = {Scientific Reports},
keywords = {PI3K,chemoresistance,lung,review},
mendeley-tags = {PI3K,chemoresistance,lung,review},
month = {dec},
number = {1},
pages = {1652},
pmid = {29374181},
title = {{Development and characterisation of a panel of phosphatidylinositide 3-kinase - Mammalian target of rapamycin inhibitor resistant lung cancer cell lines}},
url = {http://www.nature.com/articles/s41598-018-19688-1},
volume = {8},
year = {2018}
}
