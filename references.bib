issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
keywords = {CSA,gene regulation,neuritogenesis,neurology,reprogramming},
month = {oct},
number = {40},
pages = {14454--14459},
pmid = {25249633},
title = {{Dysregulation of gene expression as a cause of Cockayne syndrome neurological disease}},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1412569111},
volume = {111},
year = {2014}
}
