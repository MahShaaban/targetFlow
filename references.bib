issn = {15321827},
journal = {British Journal of Cancer},
keywords = {EMT,TBLR1,cervical cancer,cervix,clinical,epithelial-mesenchymal transition,pelvic lymph node metastasis},
mendeley-tags = {EMT,cervix,clinical},
month = {jul},
number = {1},
pages = {112--124},
pmid = {24874481},
title = {{TBLR1 is a novel prognostic marker and promotes epithelial-mesenchymal transition in cervical cancer}},
