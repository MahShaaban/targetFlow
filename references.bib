journal = {Scientific reports},
number = {1},
pages = {3137},
pmid = {28600522},
title = {{Direct competition between DNA binding factors highlights the role of Kr{\"{u}}ppel-like Factor 1 in the erythroid/megakaryocyte switch.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28600522 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5466599},
volume = {7},
year = {2017}
}
