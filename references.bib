doi = {10.1038/ng1686},
file = {:Users/mahmoudahmed/Mendeley Desktop/P{\'{a}}l, Papp, Lercher - 2005 - Adaptive evolution of bacterial metabolic networks by horizontal gene transfer.pdf:pdf},
isbn = {1061-4036},
