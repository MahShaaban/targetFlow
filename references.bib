pmid = {24395801},
title = {{Network of mutually repressive metastasis regulators can promote cell heterogeneity and metastatic transitions}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1304840111},
volume = {111},
year = {2014}
}
