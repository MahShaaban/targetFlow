journal = {American Journal of Transplantation},
keywords = {dysfunction,immune,inflammatory,ischemia reperfusion injury (IRI),lung (allograft) function,lung disease,lung transplantation,pulmonology,science,translational research},
month = {mar},
number = {3},
pages = {833--840},
pmid = {26663441},
title = {{Protein Quantitative Trait Loci Analysis Identifies Genetic Variation in the Innate Immune Regulator TOLLIP in Post-Lung Transplant Primary Graft Dysfunction Risk}},
url = {http://doi.wiley.com/10.1111/ajt.13525 http://www.ncbi.nlm.nih.gov/pubmed/26663441 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4767612},
