journal = {Journal of Clinical Pharmacy and Therapeutics},
keywords = {Body weight,Homeostasis model assessment index,Resistin,Retinol-binding protein-4,Visfatin},
month = {apr},
number = {2},
pages = {187--195},
pmid = {21812797},
title = {{Effects of 1-year orlistat treatment compared to placebo on insulin resistance parameters in patients with type 2 diabetes}},
