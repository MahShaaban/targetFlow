pmid = {28186964},
title = {{IL-6 secreted by cancer-associated fibroblasts promotes epithelial-mesenchymal transition and metastasis of gastric cancer via JAK2/STAT3 signaling pathway}},
url = {http://www.oncotarget.com/abstract/15119},
volume = {8},
year = {2017}
