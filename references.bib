issn = {17569966},
journal = {Journal of Experimental and Clinical Cancer Research},
keywords = {AKT,Cell adhesion molecule 2,Epithelial-mesenchymal transition,HCC,Hepatocellular carcinoma,MicroRNA,MicroRNA-10b,Tumor metastasis,metastasis},
mendeley-tags = {AKT,HCC,MicroRNA,metastasis},
month = {dec},
number = {1},
pages = {46},
pmid = {29506532},
title = {{CADM2, as a new target of MIR-10b, promotes tumor metastasis through FAK/AKT pathway in hepatocellular carcinoma}},
