issn = {20414889},
journal = {Cell Death and Disease},
keywords = {AKT,EMT,PI3K,neuroblastoma},
mendeley-tags = {AKT,EMT,PI3K,neuroblastoma},
month = {jan},
number = {2},
pages = {54},
pmid = {29352113},
title = {{Polo-like kinase 4 mediates epithelial-mesenchymal transition in neuroblastoma via PI3K/Akt signaling pathway article}},
url = {http://www.nature.com/articles/s41419-017-0088-2 http://www.ncbi.nlm.nih.gov/pubmed/29352113 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5833556},
volume = {9},
