issn = {14764679},
journal = {Nature Cell Biology},
number = {7},
pages = {856--863},
title = {{Molecular basis of selective mitochondrial fusion by heterotypic action between OPA1 and cardiolipin}},
volume = {19},
year = {2017}
}
