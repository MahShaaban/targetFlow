title = {{Prognostic value of microRNAs in cervical carcinoma : a systematic review and meta-analysis}},
url = {http://www.oncotarget.com/fulltext/9294 http://www.ncbi.nlm.nih.gov/pubmed/27177085 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5085235},
