pmid = {26264002},
title = {{Antitumor effects and related mechanisms of Penicitrinine A, a novel alkaloid with a unique spiro skeleton from the marine fungus Penicillium citrinum}},
url = {http://www.mdpi.com/1660-3397/13/8/4733},
