@article{Jiang2016,
abstract = {Enhancer of zeste homologue 2 (EZH2) is a potential independent mechanism for epigenetic silencing of tumor suppressor genes in cancer. We conducted an electronic search on PubMed, EMBASE, Web of Science, and Cochrane library to perform this up-to-date meta-analysis. Fifty-one studies with a total of 9444 patients were included. The prevalence of high EZH2 expression was 0.54 (95{\%} CI: 0.47-0.61). High EZH2 expression was significantly associated with poorer prognosis [overall survival: HR 1.54 (95{\%} CI: 1.30-1.78), P {\textless} 0.000; disease free survival: HR 1.35 (95{\%} CI: 1.00-1.71), P {\textless} 0.000]. In breast cancer, high EZH2 expression correlated with histological types [OR: 1.53 (95CI: 1.13-2.06); P {\textless} 0.006], histological grade [OR: 1.62 (95CI: 1.35-1.95); P {\textless} 0.000], estrogen receptor (ER) negativity [OR: 2.05 (95CI: 1.67-2.52); P {\textless} 0.000], progesterone receptor (PgR) negativity [OR: 1.42 (95CI: 1.03-1.96); P = 0.034], HER-2 positivity [OR: 1.35 (95CI: 1.08-1.69); P = 0.009], and high p53 expression [OR: 1.66 (95CI: 1.07-2.59); P = 0.024]. These results suggest that high EZH2 expression may be a promising prognostic factor to different cancers. High EZH2 expression tends to correlate with pathological types, histological grade, ER negativity, PgR negativity, HER-2 positivity and p53 high expression in breast cancer.},
author = {Jiang, Tao and Wang, Yan and Zhou, Fei and Gao, Guanghui and Ren, Shengxiang and Zhou, Caicun},
doi = {10.18632/oncotarget.6612},
issn = {1949-2553},
journal = {Oncotarget},
month = {jan},
number = {4},
pages = {4584--4597},
pmid = {26683709},
title = {{Prognostic value of high EZH2 expression in patients with different types of cancer: a systematic review with meta-analysis}},
